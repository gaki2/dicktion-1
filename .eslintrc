{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint-config-prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "import/no-unresolved": "off" ,                                                                                                                                                                                                                

    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js":"never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
   ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": ["js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
  ,
  "ignorePatterns": "dist/"
}
